<h1>New campaing</h1>

<form id="new-campaing" name="campaing">
  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value="<%= name %>" >
  </div>

  <div class="field">
    <label for="start_at"> start_at:</label>
    <input type="text"  class="datepicker" name="start_at" id="start_at" value="<%= start_at %>" >
  </div>

  <div class="field">
    <label for="end_at"> end_at:</label>
    <input type="text" class="datepicker" name="end_at" id="end_at" value="<%= end_at %>" >
  </div>

  <div class="field">
    <input type="button" id="add_countrie" value="add_countrie" >
    <div class="countries">

    </div>
  </div>

  <div class="actions">
    <input type="submit" value="Create Campaing" />
  </div>

</form>

